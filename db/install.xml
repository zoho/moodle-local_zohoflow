<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/zohoflow/db" VERSION="2025111300" COMMENT="Zoho Flow webhooks table">
    <TABLES>
        <TABLE NAME="local_zohoflow_webhooks" COMMENT="Stores webhook configurations">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" />
                <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" />
                <FIELD NAME="url" TYPE="text" NOTNULL="true" />
                <FIELD NAME="eventtype" TYPE="text" NOTNULL="true" />
                <FIELD NAME="meta" TYPE="text" NOTNULL="false" />
                <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" />
                <FIELD NAME="enabled" TYPE="int" LENGTH="1" NOTNULL="true" DEFAULT="1" />
                <FIELD NAME="signature" TYPE="char" LENGTH="255" NOTNULL="false" />
                <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" />
                <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="false" />
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id" />
            </KEYS>
        </TABLE>
    </TABLES>
</XMLDB>
